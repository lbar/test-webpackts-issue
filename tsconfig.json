{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"noEmitHelpers": true,
		"declaration": true,
		"moduleResolution": "node",
		"outDir": "./dist"
	},
	"exclude": [
		"node_modules",
		"typings/main",
		"typings/main.d.ts"
	],
	"compileOnSave": false,
	"buildOnSave": false
}
